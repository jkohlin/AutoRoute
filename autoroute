// doc ready function originally by Matteo Spinelli. 
var _el='addEventListener'_w;var _dr=false;if(document.readyState==='complete'){_dr=true;}elseif(_el){window.addEventListener('load',lo,false);}functionlo(){window.removeEventListener('load',lo,false);_dr=true;start();}
